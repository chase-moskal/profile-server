{
	"private": true,
	"name": "profiler",
	"version": "0.0.0-dev.0",
	"description": "server to manage user profile data",
	"main": "dist/main.js",
	"scripts": {
		"prepare": "run-s clean && run-p build && run-s test",
		"clean": "rm -rf dist && mkdir dist",
		"build": "tsc && run-p build-profiler-cache-script",
		"build-profiler-cache-script": "browserify dist/clientside/profiler-cache-script.js > dist/clientside/profiler-cache-script.bundle.js",
		"watch": "tsc -w",
		"generate-keys": "./node_modules/authoritarian/generate-keys && mv private.pem config/profiler.private.pem && mv public.pem config/profiler.public.pem",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/profiler.git"
	},
	"keywords": [],
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/chase-moskal/profiler/issues"
	},
	"homepage": "https://github.com/chase-moskal/profiler#readme",
	"dependencies": {
		"authoritarian": "0.0.0-dev.23",
		"crosscall": "0.0.6-dev.8",
		"fancyfs": "^0.1.0",
		"google-auth-library": "^5.2.2",
		"koa": "^2.8.1",
		"koa-bodyparser": "^4.2.1",
		"koa-mount": "^4.0.0",
		"koa-static": "^5.0.0",
		"mongodb": "^3.3.2",
		"pug": "^2.0.4",
		"renraku": "0.0.0-dev.10"
	},
	"devDependencies": {
		"@types/express": "^4.17.1",
		"@types/gapi.auth2": "^0.0.50",
		"@types/jest": "^24.0.18",
		"@types/koa": "^2.0.49",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-compose": "^3.2.4",
		"@types/koa-mount": "^4.0.0",
		"@types/koa-static": "^4.0.1",
		"@types/mongodb": "^3.3.2",
		"@types/node": "^12.7.5",
		"@types/pug": "^2.0.4",
		"browserify": "^16.5.0",
		"jest": "^24.9.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^3.6.3"
	}
}
