{
	"private": true,
	"name": "profile-server",
	"version": "0.0.0-dev.1",
	"description": "server to manage user profile data",
	"main": "dist/main.js",
	"scripts": {
		"prepare": "run-s clean && run-p build generate-keys && run-s test",
		"clean": "rm -rf dist && mkdir dist",
		"build": "tsc",
		"watch": "tsc -w",
		"generate-keys": "./node_modules/redcrypto/generate-keys && mv private.pem config/profile-server.private.pem && mv public.pem config/profile-server.public.pem",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/profile-server.git"
	},
	"keywords": [],
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/chase-moskal/profile-server/issues"
	},
	"homepage": "https://github.com/chase-moskal/profile-server#readme",
	"dependencies": {
		"@koa/cors": "^3.0.0",
		"authoritarian": "0.0.0-dev.38",
		"crosscall": "0.0.6-dev.16",
		"fancyfs": "^0.1.0",
		"google-auth-library": "^5.10.0",
		"koa": "^2.11.0",
		"koa-bodyparser": "^4.2.1",
		"koa-mount": "^4.0.0",
		"koa-static": "^5.0.0",
		"mongodb": "^3.5.3",
		"pug": "^2.0.4",
		"renraku": "0.0.0-dev.21"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/gapi.auth2": "0.0.51",
		"@types/jest": "^25.1.3",
		"@types/koa": "^2.11.1",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-compose": "^3.2.5",
		"@types/koa-mount": "^4.0.0",
		"@types/koa-static": "^4.0.1",
		"@types/koa__cors": "^3.0.1",
		"@types/mongodb": "^3.3.16",
		"@types/node": "^13.7.4",
		"@types/pug": "^2.0.4",
		"browserify": "^16.5.0",
		"jest": "^25.1.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^3.8.2"
	}
}
